  "author": "",
  "license": "AGPL-3.0",
  "dependencies": {
    "@queryleaf/lib": "0.1.0",
    "debug": "^4.4.0",
    "mongodb": "^6.14.2",
    "pg-protocol": "^1.8.0",
    "pg-query-stream": "^4.5.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/mongodb": "^4.0.7",
    "@types/pg": "^8.11.0",
    "@types/pg-query-stream": "^3.4.0",
    "@types/yargs": "^17.0.32",
    "jest": "^29.7.0",
    "mongodb-memory-server": "9.0.1",
    "pg": "^8.11.3",
    "ts-jest": "^29.2.6",
