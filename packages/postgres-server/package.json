{
  "name": "@queryleaf/postgres-server",
  "version": "0.1.0",
  "description": "PostgreSQL wire-compatible server for QueryLeaf",
  "main": "dist/pg-server.js",
  "bin": {
    "queryleaf-pg-server": "./bin/queryleaf-pg-server"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "typecheck": "tsc --noEmit",
